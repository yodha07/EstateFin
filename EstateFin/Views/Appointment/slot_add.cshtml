@model Slot
<form method="post">

    <div class="form-group">
        <label>Slot Type</label>
        <input type="text" asp-for="Slot_type" class="form-control" id="exampleInputPassword1" placeholder="Slot">
    </div>

    <div class="form-group">
        <label for="exampleFormControlSelect1">Example select</label>
        <select class="form-control" asp-for ="Status" id="exampleFormControlSelect1">
            <option>Active</option>
            <option>InActive</option>
            
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>

</form>
